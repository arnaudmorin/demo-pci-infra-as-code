# Configure the OpenStack Provider
provider "openstack" {
    user_name  = ""
    tenant_name = ""
    password  = ""
    auth_url  = "https://auth.cloud.ovh.net/v2.0/"
}
resource "openstack_compute_keypair_v2" "gw" {
  region = "GRA1"
  name = "gw"
  public_key = ""
}

module "app" {
  source = "../terraform-modules/app"
  count = 4
  application_key = ""
  application_secret = ""
  consumer_key = ""
}

module "stress" {
  source = "../terraform-modules/stress"
  count = 4
  id_keys = ""
}

